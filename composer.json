{
    "name": "yuvalkatz/greendate",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4.0",
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/dbal": ">=2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/assetic-bundle": "^2.8",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": ">=3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "craue/formflow-bundle": "~3.0@dev",
        "knplabs/knp-paginator-bundle": "^2.5",
        "jms/serializer-bundle": "^1.1",
        "friendsofsymfony/rest-bundle": "2.4.0",
        "nelmio/cors-bundle": "1.5.6",
        "nelmio/api-doc-bundle": "2.13.2",
        "suncat/mobile-detect-bundle": "1.0.*",
        "knplabs/knp-snappy-bundle" : "1.6.0",
        "knplabs/knp-menu-bundle": "^2.2.1",

        "jms/di-extra-bundle": "1.7.1",
        "jms/i18n-routing-bundle": "2.0.0",
        "knplabs/doctrine-behaviors": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.2",

        "sonata-project/admin-bundle": "^3.4-dev",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/translation-bundle": "^2.0",

        "liip/imagine-bundle": "^1.6",
        "beberlei/doctrineextensions": "^1.2",
        "symfony/var-dumper": "3.1.10"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public_html",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
